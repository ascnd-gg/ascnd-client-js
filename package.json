{
  "name": "@ascnd-gg/client",
  "version": "1.1.1",
  "description": "TypeScript/JavaScript client for the Ascnd leaderboard API",
  "author": "Ascnd",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "generate": "buf generate",
    "build": "npm run generate && tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "release": "pnpm build && changeset publish"
  },
  "dependencies": {
    "@connectrpc/connect": "^2.1.1",
    "@connectrpc/connect-web": "^2.1.1",
    "@bufbuild/protobuf": "^2.10.2"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.47.0",
    "@bufbuild/protoc-gen-es": "^2.10.2",
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.0",
    "tsup": "^8.0.0",
    "typescript": "^5.7.0"
  },
  "keywords": [
    "ascnd",
    "leaderboard",
    "game",
    "api",
    "client"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ascnd-gg/ascnd-client-js.git"
  },
  "homepage": "https://ascnd.gg/docs/sdks/typescript",
  "bugs": {
    "url": "https://github.com/ascnd-gg/ascnd-client-js/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "packageManager": "pnpm@9.15.0",
  "publishConfig": {
    "access": "public"
  }
}
